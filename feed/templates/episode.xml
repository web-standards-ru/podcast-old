<item>

    <title>{% title %}</title>
    <pubDate>{% date %}</pubDate>
    <description>
        <![CDATA[{% html %}]]>
    </description>
    <guid isPermaLink="true">https://web-standards.ru/podcast/episodes/{% N %}.mp3</guid>
    <enclosure type="audio/mpeg" url="https://web-standards.ru/podcast/episodes/{% N %}.mp3" length="{% size %}"/>

    <itunes:duration>{% duration %}</itunes:duration>
    <itunes:author>{% authors %}</itunes:author>
    <itunes:explicit>no</itunes:explicit>
    <itunes:summary>{% summary %}</itunes:summary>
    <itunes:image href="https://web-standards.ru/podcast/cover.png"/>

</item>
